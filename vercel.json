{
  "version": 2,
  "builds": [
    {
      "src": "server/src/*",
      "use": "@vercel/node"
    },
    {
      "src": "client",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
